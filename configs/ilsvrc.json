{
   "model": {
      "name":                         "resnet"
   },
   "optimizer": {
      "name":                          "adam",
      "learning_rate":                 0.001
   },
   "learning_rate": {
      "name":                          "cyclic_learning_rate",
      "cyclic_learning_rate": {
         "learning_rate":              0.00001,
         "max_lr":                     0.01,
         "step_size":                  200,
         "gamma":                      0.99994,
         "mode":                       "triangular2"
      }
   },
   "loss": {
      "name":                          "SparseCategoricalCrossentropy",
      "args": {
         "from_logits":                true
      }
   },
   "accuracy": {
      "name":                          "simple_class_accuracy"
   },
   "data": {
      "handler":                       "ilsvrc_dataset",
      "batch_size":                    10,
      "train_filelist":                "/projects/datascience/parton/git/tensorflow_skeleton/ilsvrc_train.txt",
      "test_filelist":                 "/projects/datascience/parton/git/tensorflow_skeleton/ilsvrc_test.txt",
      "shuffle_buffer":                10,
      "reshuffle_each_iteration":      true,
      "num_parallel_readers":          4,
      "prefectch_buffer_size":         10,
      "crop_image_size":               [512,512],
      "num_classes":                   1000,
      "num_channels":                  3
   },
   "training": {
      "epochs":                        100,
      "status":                        25
   },
   "validation": {
      "batches":                       1
   }
}
